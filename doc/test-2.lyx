 & =L_{k}M_{kj}\phi''_{j}+L_{k}\frac{\partial M_{kj}}{\partial q^{i}}\phi'_{i}\phi'_{j}-\frac{1}{2}L_{k}\frac{\partial M_{ij}}{\partial q^{k}}\phi'_{i}\phi'_{j}\\
 & =\alpha'-\frac{1}{2}L_{k}\frac{\partial M_{ij}}{\partial q^{k}}\phi'_{i}\phi'_{j}
\end{align*}

\end_inset

Связь
\begin_inset Formula 
\begin{align*}
\Phi_{i} & =q_{s,i}+M_{ij}^{-1}\left(q_{s}\right)B_{j}\theta+\frac{1}{2}w_{i}\theta^{2}\\
\Phi'_{i}\left(0\right) & =M_{ij}^{-1}\left(q_{s}\right)B_{j}
\end{align*}

\end_inset

Тогда
\begin_inset Formula 
\begin{align*}
\alpha & =L_{i}M_{ij}\left(\Phi\right)\left(M_{ik}^{-1}\left(q_{s}\right)B_{k}+w_{k}\theta\right)\\
\alpha\left(0\right) & =0\\
\alpha' & =L_{i}\frac{\partial M_{ij}}{\partial q_{k}}\left[M_{il}^{-1}\left(q_{s}\right)B_{l}+w_{i}\theta\right]\left[M_{kt}^{-1}\left(q_{s}\right)B_{t}+w_{k}\theta\right]+L_{i}M_{ij}w_{j}\\
\alpha'\left(0\right) & =L_{i}\frac{\partial M_{ij}}{\partial q_{k}}\left[M_{il}^{-1}\left(q_{s}\right)B_{l}\right]\left[M_{kt}^{-1}\left(q_{s}\right)B_{t}\right]+L_{i}M_{ij}\left(q_{s}\right)w_{j}\\
\beta & =L_{i}M_{ij}w_{j}+L_{i}\frac{\partial M_{ij}}{\partial q_{k}}\left[M_{kl}^{-1}\left(q_{s}\right)B_{l}+w_{k}\theta\right]\left[M_{jt}^{-1}\left(q_{s}\right)B_{t}+w_{j}\theta\right]-\frac{1}{2}L_{i}\frac{\partial M_{jk}}{\partial q_{i}}\left[M_{kl}^{-1}\left(q_{s}\right)B_{l}+w_{k}\theta\right]\left[M_{jt}^{-1}\left(q_{s}\right)B_{t}+w_{j}\theta\right]\\
\beta\left(0\right) & =L_{i}M_{ij}w_{j}+L_{i}\frac{\partial M_{ij}}{\partial q_{k}}\left[M_{kl}^{-1}\left(q_{s}\right)B_{l}\right]\left[M_{jt}^{-1}\left(q_{s}\right)B_{t}\right]-\frac{1}{2}L_{i}\frac{\partial M_{jk}}{\partial q_{i}}\left[M_{kl}^{-1}\left(q_{s}\right)B_{l}\right]\left[M_{jt}^{-1}\left(q_{s}\right)B_{t}\right]\\
 & =\alpha'\left(0\right)-\frac{1}{2}L_{i}\frac{\partial M_{jk}}{\partial q_{i}}\left[M_{kl}^{-1}\left(q_{s}\right)B_{l}\right]\left[M_{jt}^{-1}\left(q_{s}\right)B_{t}\right]
\end{align*}

